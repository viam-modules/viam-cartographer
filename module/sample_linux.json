        "type": "camera",
        "depends_on": []
      }
    ],
    "services": [
      {
        "name": "slam-test",
        "model": "viam:slam:cartographer",
        "type": "slam",
        "attributes": {
          "port": "localhost:8083",
          "data_dir": "/Users/kat/viam/datasets/test-delete-me",
